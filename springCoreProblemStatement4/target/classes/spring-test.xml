<?xml version="1.0" encoding="UTF-8"?>


<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	
	<bean id="student" class="com.taufeeque.springCoreProblemStatement4.Student">
		<property name="studentName" value="Md Taufeeque Alam" />
		<property name="rollNo" value="46" />
		<property name="address" ref="address"></property>
	</bean>
	
	<bean id="address" class="com.taufeeque.springCoreProblemStatement4.Address">
		<property name="street" value="S.R Nagar" />
		<property name="city" value="Hyderabad" />
		<property name="state" value="Telangana" />
	</bean>
	
</beans>